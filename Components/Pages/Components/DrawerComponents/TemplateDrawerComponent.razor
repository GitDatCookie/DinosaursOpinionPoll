<MudListItem T="string" Text="@Title" Icon="@Icon" OnClick="HandleOnClick" />

@code {
    [Parameter]
    public EventCallback<(EItemType, EQuestionType)> OnTemplateSelected { get; set; }

    [Parameter]
    public EQuestionType QuestionType { get; set; }

    [Parameter] 
    public EItemType ItemType { get; set; }

    [Parameter]
    public string Icon { get; set; }

    [Parameter]
    public string Title { get; set; }


    private async Task HandleOnClick()
    {
        await OnTemplateSelected.InvokeAsync((ItemType, QuestionType));
    }
}